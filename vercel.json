{
    "version": 2,
    "builds": [
      {
        "src": "dist/main.js",
        "use": "@vercel/node"
      },
      {
        "src": "../frontend/build",
        "use": "@vercel/static-build",
        "config": {
          "distDir": "public"
        }
      }
    ],
    "routes": [
      {
        "src": "/users/(.*)",
        "dest": "/src/main.ts"
      },
      {
        "src": "/expeditions/(.*)",
        "dest": "/src/main.ts"
      },
      {
        "src": "/(.*)",
        "dest": "/public/$1"
      }
    ]
  }
{
    "version": 2,
    "builds": [
      {
        "src": "dist/main.js",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/users/(.*)",
        "dest": "/dist/main.js"
      },
      {
        "src": "/expeditions/(.*)",
        "dest": "/dist/main.js"
      },
      {
        "src": "/(.*)",
        "dest": "/public/$1"
      }
    ]
  }